(defun dotspacemacs/user-load ()
  "Library to load while dumping.
This function is called only while dumping Spacemacs configuration. You can
`require' or `load' the libraries of your choice that will be included in the
dump."
  )


(defun dotspacemacs/user-config ()
  "Configuration function for user code.
This function is called at the very end of Spacemacs initialization after
layers configuration.
This is the place where most of your configurations should be done. Unless it is
explicitly specified that a variable should be set before a package is loaded,
you should place your code here."
  ;; keyboard remaps
  (define-key evil-normal-state-map (kbd "TAB") 'evil-toggle-fold)

  ;; package configs
  ;; lsp config
  (setq lsp-ui-doc-enable nil)

  ;; not sure what this is for but i think forge lol
  (setq auth-sources '("~/.authinfo.gpg"))
  ;; projectile config
  (setq projectile-project-search-path '("~/git/" "~/godot/"))

  ;; gdscript config
  (require 'gdscript-mode)
  (add-hook 'gdscript-mode-hook (lambda() (lsp)))

  ;; c-c++ mode
  (add-hook 'c++-mode-hook (lambda() (indent-tabs-mode t)))

  ;; org config
  (add-hook 'org-mode-hook (lambda () (visual-line-mode 1)))
  (global-set-key "\C-ci" 'org-roam-node-insert)

  ;; org roam config
  ;;(org-roam-setup)
  ;;(define-key org-roam-mode-map (kbd "RET") (kbd "C-0 <C-return>"))
  ;;(define-key org-roam-mode-map [mouse-1] #'org-roam-visit-thing)
  (add-hook 'org-roam-mode-hook (lambda () ;;(page-break-lines-mode 0)
                                  ;;(org-roam-buffer-render) ;; sadly doesn't work seems you need to set global-page-break-lines-mode
                                  (local-unset-key (kbd "h"))
                                  (local-unset-key (kbd "SPC"))
                                  ))

  ;; disable helm-ls-git-rebase-todo-mode
  (setq auto-mode-alist (delete '("/git-rebase-todo$" . helm-ls-git-rebase-todo-mode) auto-mode-alist))

  ;; theme config
  (setq doom-themes-treemacs-theme "doom-atom")
  (doom-themes-treemacs-config)

  ;; transparent background
  (defun on-frame-open (&optional frame)
    "If the FRAME created in terminal don't load background color."
    (unless (display-graphic-p frame)
      (set-face-background 'default "unspecified-bg" frame)))
  (add-hook 'after-make-frame-functions 'on-frame-open)
  (add-hook 'after-make-frame-functions 'spacemacs/enable-background-transparency)

  ;; Custom commands
  (spacemacs/declare-prefix "o" "custom")
  (spacemacs/set-leader-keys "or" 'ranger)
  ;; Probably temporary shortcuts for new roam
  (spacemacs/declare-prefix "od" "org-roam")
  (spacemacs/set-leader-keys "odt" 'org-roam-dailies-capture-today)
  (spacemacs/set-leader-keys "odg" 'org-roam-dailies-goto-today)
  (spacemacs/set-leader-keys "odc" 'org-roam-capture)
  (spacemacs/set-leader-keys "odf" 'org-roam-node-find)
  (spacemacs/set-leader-keys "odl" 'org-roam-buffer-toggle)
  (spacemacs/declare-prefix "os" "switch mode")
  (spacemacs/set-leader-keys "osh" 'html-mode)
  (spacemacs/set-leader-keys "osj" 'javascript-mode)

  (use-package intuitive-tab-line
    :load-path "private/pkgs/intuitive-tab-line-mode"
    :custom
    ;; (tab-line-tabs-function 'intuitive-tab-line-buffers-list)
    (tab-line-switch-cycling t)
    :config
    (global-tab-line-mode 1)
    (recentf-mode 1)
    (setq
     tab-line-new-button-show nil  ;; do not show add-new button
     tab-line-close-button-show nil  ;; do not show close button
     tab-line-separator " "  ;; delimitation between tabs
     ))
  (global-set-key (kbd "C-h") 'tab-line-switch-to-prev-tab)
  (global-set-key (kbd "C-l") 'tab-line-switch-to-next-tab)
  (global-set-key (kbd "C-<tab>") 'tab-line-switch-to-next-tab)

  ;; custom function to open a project from cli
  (defun open-project-magit-in-layout (project-path layout-name)
    "Open the project at PROJECT-PATH in a new Spacemacs layout named LAYOUT-NAME and open the Magit status buffer."
    (interactive "DProject Path: \nsLayout Name: ")
    ;; Create or switch to the layout
    (persp-switch layout-name)
    ;; Ensure the project is added to Projectile's known projects
    (unless (member project-path projectile-known-projects)
      (projectile-add-known-project project-path))
    ;; Switch to the project's directory
    (let ((default-directory project-path))
      ;; Open the Magit status buffer
      (magit-status)))

  ;; close compilation buffer if it is finished without errors
  (defvar user-compile-process nil
    "The current compilation process or nil if none.")

  (defun user-bury-buffer (window buffer)
    "Bury the BUFFER and switch to the previous buffer in WINDOW, ignoring
errors.

If the previous buffer cannot be found for the WINDOW, then it will simply
delete the WINDOW."
    (ignore-errors
      (unless (switch-to-prev-buffer window)
        (delete-window window))
      (bury-buffer buffer)))

  (defun user-compile-start (proc)
    (when (string-match "*.*compilation*" (buffer-name (current-buffer)))
      (setq user-compile-process proc)))

  (defun user-compile-done (buffer _msg)
    (when (string-equal "*rustic-compilation*" (buffer-name buffer))
      (let* ((exit-status (process-exit-status user-compile-process))
             (has-errors  (/= 0 exit-status))
             (window (get-buffer-window buffer)))

        (when (and window (not has-errors))
          (run-at-time "1 sec" nil #'user-bury-buffer window buffer)))

      (setq user-compile-process nil)))

  (add-hook 'compilation-finish-functions #'user-compile-done)
  (add-hook 'compilation-start-hook #'user-compile-start)
  )

