 '(display-buffer-alist
   '(("\\*org-roam\\*"
      (display-buffer-in-direction)
      (direction . right)
      (window-width . 0.25)
      (window-height . fit-window-to-buffer))))
 '(evil-shift-width 4)
 '(evil-want-Y-yank-to-eol nil)
 '(global-linum-mode t)
 '(global-page-break-lines-mode nil nil (page-break-lines))
 '(hl-todo-keyword-faces
   '(("TODO" . "#dc752f")
     ("NEXT" . "#dc752f")
     ("THEM" . "#2d9574")
